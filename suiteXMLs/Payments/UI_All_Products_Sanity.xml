<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Products Sanity" parallel="tests" thread-count="1">

<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="test.java.common.RemoveDuplicateResults"/>
		<listener class-name="test.java.common.RetryListener"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />

</listeners>

    <test name="Inline PayTM Tokenized Card" preserve-order="true">
        <classes>
            <class name="test.java.paymentBento_Air_Desktop.IN_Air_Desktop_CC_PayTM_Inline_TokenizeCard"/>
        </classes>
    </test>


    <test name="Paylater ICICI" preserve-order="true">
        <classes>
            <class name="test.java.paymentsBento_Itn_Air_PWA.IN_Air_PWA_Paylater"/>
        </classes>
    </test>


    <test name="Coupon Valid Razorpay" preserve-order="true">
        <classes>
            <class name="test.java.paymentsBento_Itn_Hotels.IN_Coupon_Valid"/>
        </classes>
    </test>


</suite>