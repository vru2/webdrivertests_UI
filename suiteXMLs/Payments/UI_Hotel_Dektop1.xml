<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Hotel Booking" parallel="tests" thread-count="2">

<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="test.java.common.RemoveDuplicateResults"/>
		<listener class-name="test.java.common.RetryListener"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
</listeners>

    <test name="Coupon Valid Razorpay" preserve-order="true">
        <classes>
            <class name="test.java.paymentsBento_Itn_Hotels.IN_Coupon_Valid"/>
        </classes>
    </test>


    <test name="GV Price Change PopUP NB" preserve-order="true">
        <classes>
            <class name="test.java.paymentsBento_Itn_Hotels.IN_GV_Price_Change_PopUp"/>
        </classes>
    </test>

</suite>